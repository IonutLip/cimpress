<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>

        <defaults autowire="true" autoconfigure="true" public="false" />

        <service id="cimpress.prepress" class="Printi\CimpressBundle\Services\CimpressPrepress">
            <argument>%credentials%</argument>
            <argument>%prepress_client_id%</argument>
        </service>
        <service id="Printi\CimpressBundle\Services\CimpressPrepress" alias="cimpress.prepress" />

        <service id="cimpress.pdf.processing" class="Printi\CimpressBundle\Services\CimpressPdfProcessing">
            <argument>%credentials%</argument>
            <argument>%pdf_processing_client_id%</argument>
        </service>
        <service id="Printi\CimpressBundle\Services\CimpressPdfProcessing" alias="cimpress.pdf.processing" />

    </services>
</container>
